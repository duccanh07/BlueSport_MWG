@model List<BlueSport_MVC.Models.ListBrandModel>
<div class="trademark__list owl-carousel">
    @foreach (var item in Model)
    {
        <div class="item">
            @foreach (var brand in item.BrandModel)
            {
                <p><img src="@brand.ImageUrl" alt="@brand.ImageName" width="@brand.width" height="@brand.height"></p>
            }
        </div>
    }
</div>
